<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegistrationUser</servlet-name>
  	<display-name>RegistrationUser</display-name>
  	<description></description>
  	<servlet-class>Registration.RegistrationUser</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AuthenticationServlet</servlet-name>
  	<display-name>AuthenticationServlet</display-name>
  	<description></description>
  	<servlet-class>user.profile.AuthenticationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TestAuth</servlet-name>
  	<display-name>TestAuth</display-name>
  	<description></description>
  	<servlet-class>user.profile.TestAuth</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DataUserEditedServlet</servlet-name>
  	<display-name>DataUserEditedServlet</display-name>
  	<description></description>
  	<servlet-class>user.profile.dao.DataUserEditedServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegistrationUser</servlet-name>
  	<url-pattern>/RegistrationUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AuthenticationServlet</servlet-name>
  	<url-pattern>/AuthenticationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TestAuth</servlet-name>
  	<url-pattern>/TestAuth</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DataUserEditedServlet</servlet-name>
  	<url-pattern>/DataUserEditedServlet</url-pattern>
  </servlet-mapping>
  
</web-app>
